const express = require('express');
const app = express();
app.set('view engine', 'ejs');

app.get('/', (req, res) => {
  res.render('home', { title: 'EJS Example', name: 'Akshitha' });
});

app.listen(3000, () => console.log('Server running on http://localhost:3000'));



<h2><%= title %></h2>
<p>Welcome, <%= name %>!</p>

